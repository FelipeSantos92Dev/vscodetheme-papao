  "semanticTokenColors": {
    "parameter": {
      "foreground": "#8250df"
    },
    "parameter.declaration": {
      "foreground": "#efb963"
    },
    "property": {
      "foreground": "#953800"
    },
    "property.declaration": {
      "foreground": "#0550ae"
    },
    "property.defaultLibrary": {
      "foreground": "#0e97aa"
    },
    "*.defaultLibrary": {
      "foreground": "#0e97aa"
    },
    "variable": {
      "foreground": "#0550ae"
    },
    "variable.declaration": {
      "foreground": "#8250df"
    },
    "variable.defaultLibrary": {
      "foreground": "#0e97aa"
    },
    "class.declaration": {
      "foreground": "#953800"
    },
    "class.decorator": {
      "foreground": "#8250df"
    },
    "enumMember": {
      "foreground": "#953800"
    },
    "meta.decorator": {
      "foreground": "#8250df"
    },
    "namespace": {
      "foreground": "#1c75bc"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e7a9090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#0550ae"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#0a3069"
      }
    },
    {
      "name": "String Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#0e97aa"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1c75bc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#8250df"
      }
    },
    {
      "name": "Classes, Types, and Interfaces",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#0550ae"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#8250df"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Tags (HTML, XML, etc.)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#1c75bc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#0550ae",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#0550ae",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#0e97aa"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#0550ae"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
    {
      "name": "JSON String Values",
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#0a3069"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#6e7a90"
      }
    },
    {
      "name": "String Interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#0e97aa"
      }
    },
    {
      "name": "This, Super",
      "scope": [
        "variable.language.this",
        "variable.language.special.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#953800",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import, Export, From",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#0e97aa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key",
        "string.alias.graphql",
        "string.unquoted.graphql",
        "string.unquoted.alias.graphql",
        "meta.field.declaration.ts variable.object.property",
        "meta.block entity.name.label"
      ],
      "settings": {
        "foreground": "#0550ae"
      }
    },
    {
      "name": "CSS Media Features",
      "scope": ["support.type.property-name.media.css"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
    {
      "name": "YAML Keys",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#1c75bc"
      }
    },
